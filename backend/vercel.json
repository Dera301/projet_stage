{
  "version": 3,
  "framework": "node",
  "installCommand": "npm install",
  "buildCommand": "echo no-build",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "src/server.js"
    }
  ],
  "env": {
    "PRISMA_GENERATE_DATAPROXY": "false",
    "NODE_ENV": "production"
  }
}
